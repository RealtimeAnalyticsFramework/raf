  <logRotator>
    <daysToKeep>7</daysToKeep>
    <numToKeep>-1</numToKeep>
    <artifactDaysToKeep>-1</artifactDaysToKeep>
    <artifactNumToKeep>-1</artifactNumToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties>
    <com.buildcoin.plugins.jenkins.BuildcoinProperty plugin="buildcoin-plugin@1.4">
      <buildcoinJobKey></buildcoinJobKey>
      <buildcoinJobDisabled>false</buildcoinJobDisabled>
    </com.buildcoin.plugins.jenkins.BuildcoinProperty>
    <com.sonyericsson.jenkins.plugins.bfa.model.ScannerJobProperty plugin="build-failure-analyzer@1.4.0">
      <doNotScan>false</doNotScan>
    </com.sonyericsson.jenkins.plugins.bfa.model.ScannerJobProperty>
    <org.jenkins.ci.plugins.html5__notifier.JobPropertyImpl plugin="html5-notifier-plugin@1.2">
      <skip>false</skip>
    </org.jenkins.ci.plugins.html5__notifier.JobPropertyImpl>
    <hudson.plugins.throttleconcurrents.ThrottleJobProperty plugin="throttle-concurrents@1.7.2">
      <maxConcurrentPerNode>1</maxConcurrentPerNode>
      <maxConcurrentTotal>1</maxConcurrentTotal>
      <categories>
        <string>idgs</string>
      </categories>
      <throttleEnabled>true</throttleEnabled>
      <throttleOption>category</throttleOption>
    </hudson.plugins.throttleconcurrents.ThrottleJobProperty>
  </properties>
  <scm class="hudson.scm.CVSSCM" plugin="cvs@2.7">
    <flatten>false</flatten>
    <repositories>
      <hudson.scm.CvsRepository>
        <cvsRoot>/var/cvsroot</cvsRoot>
        <repositoryItems>
          <hudson.scm.CvsRepositoryItem>
            <modules>
              <hudson.scm.CvsModule>
                <localName>idgs</localName>
                <remoteName>idgs</remoteName>
              </hudson.scm.CvsModule>
            </modules>
            <location class="hudson.scm.CvsRepositoryLocation$HeadRepositoryLocation">
              <locationType>HEAD</locationType>
              <useHeadIfNotFound>false</useHeadIfNotFound>
            </location>
          </hudson.scm.CvsRepositoryItem>
        </repositoryItems>
        <compressionLevel>-1</compressionLevel>
        <excludedRegions>
          <hudson.scm.ExcludedRegion>
            <pattern></pattern>
          </hudson.scm.ExcludedRegion>
        </excludedRegions>
        <passwordRequired>false</passwordRequired>
      </hudson.scm.CvsRepository>
    </repositories>
    <canUseUpdate>true</canUseUpdate>
    <skipChangeLog>false</skipChangeLog>
    <pruneEmptyDirectories>true</pruneEmptyDirectories>
    <disableCvsQuiet>false</disableCvsQuiet>
    <cleanOnFailedUpdate>true</cleanOnFailedUpdate>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector">
    <hudson.triggers.TimerTrigger>
      <spec>30 8 * * *</spec>
    </hudson.triggers.TimerTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>date +&quot;%Y-%m-%d %H:%M&quot;  &gt;build.timestamp</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command># 
# debug build
#
BUILD_DIR=$WORKSPACE/gcc_debug
export BUILD_DIR

if test ! -d &quot;$BUILD_DIR&quot;  ; then
  mkdir $BUILD_DIR
fi

cd $BUILD_DIR
/bin/bash $WORKSPACE/idgs/build/build.sh

cd $WORKSPACE/idgs
if test ! -d &quot;html&quot;  ; then
  /bin/bash $WORKSPACE/idgs/build/gen-apidoc.sh
fi

export GTEST_COLOR=yes

# 
# UT, IT and PT
#
/bin/bash $WORKSPACE/idgs/build/runtest.sh
/bin/bash $WORKSPACE/idgs/build/it.sh
/bin/bash $WORKSPACE/idgs/build/tpch-it.sh
/bin/bash $WORKSPACE/idgs/build/ssb-it.sh</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>#
# release build
#
BUILD_DIR=$WORKSPACE/gcc_release
export BUILD_DIR
if test ! -d &quot;$BUILD_DIR&quot;  ; then
  mkdir $BUILD_DIR
fi

CXXFLAGS=&quot;-g -O3 -DNDEBUG&quot;
export CXXFLAGS

cd $BUILD_DIR
if test ! -f &quot;Makefile&quot;; then
  ../idgs/configure --prefix=$BUILD_DIR/dist 1&gt;/dev/null
fi

echo &quot;make&quot;
make -j 8 1&gt;/dev/null || make -j 4 1&gt;/dev/null || make -j 4 1&gt;/dev/null || make -j 4 1&gt;/dev/null || make -j 4 1&gt;/dev/null || make -j 3 1&gt;/dev/null || make -j 2 1&gt;/dev/null || make 1&gt;/dev/null || make || exit $?
</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>mv build.timestamp lastsuccess.timestamp</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.plugins.doxygen.DoxygenArchiver plugin="doxygen@0.11">
      <doxyfilePath>idgs/build/doxygen.conf</doxyfilePath>
      <keepAll>false</keepAll>
      <folderWhereYouRunDoxygen>idgs</folderWhereYouRunDoxygen>
    </hudson.plugins.doxygen.DoxygenArchiver>
    <htmlpublisher.HtmlPublisher plugin="htmlpublisher@1.2">
      <reportTargets>
        <htmlpublisher.HtmlPublisherTarget>
          <reportName>Integration Test Case Log</reportName>
          <reportDir>idgs/it_log</reportDir>
          <reportFiles>index.html</reportFiles>
          <keepAll>true</keepAll>
          <wrapperName>htmlpublisher-wrapper.html</wrapperName>
        </htmlpublisher.HtmlPublisherTarget>
      </reportTargets>
    </htmlpublisher.HtmlPublisher>
    <hudson.tasks.Mailer plugin="mailer@1.4">
      <recipients>ting.lou@intel.com hongbinx.zhang@intel.com xi.tang@intel.com deyinx.chen@intel.com jingfeix.ma@intel.com</recipients>
      <dontNotifyEveryUnstableBuild>false</dontNotifyEveryUnstableBuild>
      <sendToIndividuals>false</sendToIndividuals>
    </hudson.tasks.Mailer>
    <hudson.plugins.cigame.GamePublisher plugin="ci-game@1.19"/>
  </publishers>
  <buildWrappers>
    <hudson.plugins.build__timeout.BuildTimeoutWrapper plugin="build-timeout@1.11">
      <timeoutMinutes>60</timeoutMinutes>
      <failBuild>true</failBuild>
      <writingDescription>true</writingDescription>
      <timeoutPercentage>0</timeoutPercentage>
      <timeoutType>absolute</timeoutType>
      <timeoutMinutesElasticDefault>3</timeoutMinutesElasticDefault>
    </hudson.plugins.build__timeout.BuildTimeoutWrapper>
    <hudson.plugins.ansicolor.AnsiColorBuildWrapper plugin="ansicolor@0.3.1">
      <colorMapName>xterm</colorMapName>
    </hudson.plugins.ansicolor.AnsiColorBuildWrapper>
  </buildWrappers>
</project>