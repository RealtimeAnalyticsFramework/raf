<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>&lt;b&gt;Intel Data Grid Server Integration Test&lt;/b&gt;&lt;br/&gt;&#xd;
&#xd;
</description>
  <logRotator>
    <daysToKeep>30</daysToKeep>
    <numToKeep>-1</numToKeep>
    <artifactDaysToKeep>-1</artifactDaysToKeep>
    <artifactNumToKeep>-1</artifactNumToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties>
    <com.buildcoin.plugins.jenkins.BuildcoinProperty plugin="buildcoin-plugin@1.4">
      <buildcoinJobKey></buildcoinJobKey>
      <buildcoinJobDisabled>false</buildcoinJobDisabled>
    </com.buildcoin.plugins.jenkins.BuildcoinProperty>
    <com.sonyericsson.jenkins.plugins.bfa.model.ScannerJobProperty plugin="build-failure-analyzer@1.4.0">
      <doNotScan>false</doNotScan>
    </com.sonyericsson.jenkins.plugins.bfa.model.ScannerJobProperty>
    <org.jenkins.ci.plugins.html5__notifier.JobPropertyImpl plugin="html5-notifier-plugin@1.2">
      <skip>false</skip>
    </org.jenkins.ci.plugins.html5__notifier.JobPropertyImpl>
    <hudson.plugins.throttleconcurrents.ThrottleJobProperty plugin="throttle-concurrents@1.7.2">
      <maxConcurrentPerNode>1</maxConcurrentPerNode>
      <maxConcurrentTotal>1</maxConcurrentTotal>
      <categories>
        <string>idgs</string>
      </categories>
      <throttleEnabled>true</throttleEnabled>
      <throttleOption>category</throttleOption>
    </hudson.plugins.throttleconcurrents.ThrottleJobProperty>
  </properties>
  <scm class="hudson.scm.CVSSCM" plugin="cvs@2.7">
    <flatten>false</flatten>
    <repositories>
      <hudson.scm.CvsRepository>
        <cvsRoot>/var/cvsroot</cvsRoot>
        <repositoryItems>
          <hudson.scm.CvsRepositoryItem>
            <modules>
              <hudson.scm.CvsModule>
                <localName>idgs</localName>
                <remoteName>idgs</remoteName>
              </hudson.scm.CvsModule>
            </modules>
            <location class="hudson.scm.CvsRepositoryLocation$HeadRepositoryLocation">
              <locationType>HEAD</locationType>
              <useHeadIfNotFound>false</useHeadIfNotFound>
            </location>
          </hudson.scm.CvsRepositoryItem>
        </repositoryItems>
        <compressionLevel>-1</compressionLevel>
        <excludedRegions>
          <hudson.scm.ExcludedRegion>
            <pattern></pattern>
          </hudson.scm.ExcludedRegion>
        </excludedRegions>
        <passwordRequired>false</passwordRequired>
      </hudson.scm.CvsRepository>
    </repositories>
    <canUseUpdate>true</canUseUpdate>
    <skipChangeLog>false</skipChangeLog>
    <pruneEmptyDirectories>true</pruneEmptyDirectories>
    <disableCvsQuiet>false</disableCvsQuiet>
    <cleanOnFailedUpdate>true</cleanOnFailedUpdate>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector">
    <hudson.triggers.TimerTrigger>
      <spec>*/10 0-7 * * *
*/10 18-23 * * *
*/10 * * * 6-7
</spec>
    </hudson.triggers.TimerTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>date +&quot;%Y-%m-%d %H:%M&quot;  &gt;build.timestamp
# 
# debug build
#
BUILD_DIR=$WORKSPACE/gcc_debug
export BUILD_DIR

rm -f $WORKSPACE/idgs/*.txt

if test ! -d &quot;$BUILD_DIR&quot;  ; then
  mkdir $BUILD_DIR
fi

cd $BUILD_DIR
/bin/bash $WORKSPACE/idgs/build/build.sh

# 
# UT, IT and PT
#
export GTEST_COLOR=yes

/bin/bash $WORKSPACE/idgs/build/runtest.sh
/bin/bash $WORKSPACE/idgs/build/it.sh
#/bin/bash $WORKSPACE/idgs/build/tpch-it.sh
#/bin/bash $WORKSPACE/idgs/build/ssb-it.sh</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>#
# release build
#
rm -f $WORKSPACE/idgs/*.txt

BUILD_DIR=$WORKSPACE/gcc_release
export BUILD_DIR
if test ! -d &quot;$BUILD_DIR&quot;  ; then
  mkdir $BUILD_DIR
fi

LD_PRELOAD=/usr/local/lib/libtcmalloc.so
export LD_PRELOAD
CXXFLAGS=&quot;-g -O3 -DNDEBUG&quot;
export CXXFLAGS

cd $BUILD_DIR
/bin/bash $WORKSPACE/idgs/build/build.sh

export GTEST_COLOR=yes

cd $WORKSPACE/idgs
/bin/bash $WORKSPACE/idgs/build/tpch.sh
/bin/bash $WORKSPACE/idgs/build/ssb.sh</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>mv build.timestamp lastsuccess.timestamp</command>
    </hudson.tasks.Shell>
  </builders>
