find . -name "*.cpp" -exec sed '1 i\\' -i {} \;
find . -name "*.cpp" -exec sed '1r template' -i {} \;

find . -name "*.h" -exec sed '1 i\\' -i {} \;
find . -name "*.h" -exec sed '1r template' -i {} \;


find . -name "*.java" -exec sed '1 i\\' -i {} \;
find . -name "*.java" -exec sed '1r template' -i {} \;
